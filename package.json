{
  "name": "lyon-marquage-service",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "db:local:migrate": "prisma migrate dev",
    "db:local:studio": "prisma studio",
    "db:local:push": "prisma db push",
    "db:local:clear": "npx ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/clearData.ts",
    "db:local:seed": "prisma db seed",
    "db:prod:migrate": "NODE_ENV=production prisma migrate deploy",
    "db:prod:status": "NODE_ENV=production prisma migrate status",
    "db:prod:push": "NODE_ENV=production prisma db push --accept-data-loss",
    "vercel:env:pull": "vercel env pull .env.production.local",
    "update-catalogue": "ts-node scripts/updateCatalogueMenu.ts",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "prisma": {
    "seed": "npx ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
