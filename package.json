{
  "name": "lyon-marquage-service",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "db:local:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:local:pull": "dotenv -e .env.local -- prisma db pull",
    "db:local:studio": "dotenv -e .env.local -- prisma studio",
    "db:local:push": "dotenv -e .env.local -- prisma db push",
    "db:local:clear": "dotenv -e .env.local -- tsx prisma/clearData.ts",
    "db:local:seed": "dotenv -e .env.local -- prisma db seed",
    "db:local:update": "dotenv -e .env.local -- tsx prisma/update.ts",
    "db:prod:migrate": "NODE_ENV=production prisma migrate deploy",
    "db:prod:migrate:debug": "NODE_ENV=production DEBUG=\"prisma:*\" prisma migrate deploy",
    "db:prod:migrateFile": "NODE_ENV=production tsx scripts//migrateImageRelationships.ts",
    "db:prod:clear": "NODE_ENV=production tsx prisma/clearData.ts",
    "db:prod:seed": "NODE_ENV=production prisma db seed",
    "db:prod:update": "NODE_ENV=production tsx prisma/update.ts",
    "db:prod:status": "NODE_ENV=production prisma migrate status",
    "db:prod:push": "NODE_ENV=production prisma db push --accept-data-loss",
    "vercel:env:pull": "vercel env pull .env.production.local",
    "update-catalogue": "tsx scripts/updateCatalogueMenu.ts",
    "generate-catalogue": "tsx scripts/generateCataloguePages.ts",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.4",
    "@hcaptcha/react-hcaptcha": "^1.12.0",
    "@hookform/resolvers": "^3.10.0",
    "@prisma/client": "^6.3.1",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.3",
    "@radix-ui/react-progress": "^1.1.6",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.1.2",
    "@react-icons/all-files": "^4.1.0",
    "@supabase/supabase-js": "^2.49.4",
    "axios": "^1.8.3",
    "bcryptjs": "^3.0.2",
    "busboy": "^1.6.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "embla-carousel-react": "^8.5.2",
    "install": "^0.13.0",
    "lucide-react": "^0.454.0",
    "next": "14.2.23",
    "next-themes": "^0.4.4",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.5.0",
    "sharp": "^0.34.1",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/busboy": "^1.5.4",
    "@types/gtag.js": "^0.0.20",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.12",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/sharp": "^0.32.0",
    "@types/uuid": "^10.0.0",
    "embla-carousel-autoplay": "^8.5.2",
    "eslint": "^8",
    "eslint-config-next": "15.0.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock-extended": "4.0.0-beta1",
    "next-auth": "5.0.0-beta.25",
    "nodemailer": "^6.10.0",
    "postcss": "^8",
    "prettier": "3.5.3",
    "prisma": "^6.3.1",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3"
  }
}
